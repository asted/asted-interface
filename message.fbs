include "tree.fbs";

namespace ASTEd.Interface;

table InitRequest {
	lang:string (required);
}

table FileRequest {
	path:string (required);
}

table FileResponse {
	tree:ASTEd.Interface.Tree.Node;
}

union RequestUnion {
	InitRequest,
	FileRequest,
}

table Request {
	request:RequestUnion (required);
}

root_type Request;
